{
  "functions": {
    "app/api/dodo/webhook/route.ts": {
      "maxDuration": 30
    },
    "app/api/paypal/webhook/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/dodo/webhook",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, webhook-id, webhook-signature, webhook-timestamp"
        }
      ]
    },
    {
      "source": "/api/paypal/webhook",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, paypal-auth-algo, paypal-cert-id, paypal-transmission-id, paypal-transmission-sig, paypal-transmission-time"
        }
      ]
    }
  ]
}
